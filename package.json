{
  "name": "msgpackstream",
  "version": "1.0.0",
  "description": "A stream-encoding/decoding version of msgpack that uses string tables to transfer data",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "buildwatch": "tsc -w",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && gut add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --config jestconfig.json"
  },
  "keywords": [
    "msgpack",
    "streamencoding"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chibisuke/msgpackstream.git"
  },
  "author": "Chibisuke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chibisuke/msgpackstream/issues"
  },
  "homepage": "https://github.com/chibisuke/msgpackstream#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "jest": "^27.2.3",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3",
    "notepack.io": "^2.3.0"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@msgpack/msgpack": "^2.7.1",
    "benchtable": "^0.1.0",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "msgpack-js": "^0.3.0",
    "msgpack-lite": "^0.1.26",
    "msgpack5": "^5.3.2"
  }
}
